<?php

/**
 * Implements hook_page_attachments().
 */
function webmaster_local_staff_page_attachments(array &$attachments) {
  // Get current route.
  $route_name = \Drupal::routeMatch()->getRouteName();

  // Check for your specific route.
  if ($route_name === 'view.staff.page_1') {
    $attachments['#attached']['library'][] = 'webmaster_local_staff/webmaster_local_staff';
  }

}

